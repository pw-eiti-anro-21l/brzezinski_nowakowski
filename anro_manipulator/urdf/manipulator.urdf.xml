
<robot name="manipulator">

  <link name="base">
    <visual>
      <origin xyz="0 0 0.05" rpy="0 0 0" />
      <geometry>
        <box size="0.2 0.3 0.1" />
      </geometry>
      <material name="yellow">
        <color rgba="0 .5 .5 1" />
      </material>
    </visual>
  </link>

  <link name="cylinder">
    <visual>
      <origin xyz="0 0 0.05" rpy="0 0 0" />
      <geometry>
        <cylinder length="0.1" radius="0.1" />
      </geometry>
      <material name="red">
        <color rgba=".5 0 0 1" />
      </material>
    </visual>
  </link>

  <joint name="base-cyl" type="fixed">
    <parent link="base"/>
    <child link="cylinder"/>
    <origin xyz="0 0 0.1"/>
  </joint>  

  <link name="arm1">
    <visual>
      <origin xyz="0 0 0.4" rpy="0 0 0" />
      <geometry>
        <box size="0.1 0.1 0.8" />
      </geometry>
      <material name="green">
        <color rgba="0 .5 0 1" />
      </material>
    </visual>
  </link>

  <joint name="cyl-arm1" type="fixed">
    <parent link="cylinder"/>
    <child link="arm1"/>
    <origin xyz="0 0 0.2"/>
  </joint>

  <link name="arm2">
    <visual>
      <origin xyz="0 0 1.35" rpy="0 0 0" />
      <geometry>
        <box size="0.08 0.08 0.7" />
      </geometry>
      <material name="blue">
        <color rgba="0 0 0.5 1" />
      </material>
    </visual>
  </link>

  <joint name="arm1-arm2" type="fixed">
    <parent link="arm1"/>
    <child link="arm2"/>
  </joint>

  <link name="arm3">
    <visual>
      <origin xyz="0 0 1.85" rpy="0 0 0" />
      <geometry>
        <box size="0.06 0.06 0.3" />
      </geometry>
      <material name="purple">
        <color rgba="1 0 0.5 1" />
      </material>
    </visual>
  </link>

  <joint name="arm2-arm3" type="fixed">
    <parent link="arm2"/>
    <child link="arm3"/>
  </joint>


